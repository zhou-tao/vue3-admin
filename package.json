{
  "name": "vue3-starter",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": "^14.18.0 || >=16.0.0"
  },
  "scripts": {
    "dev": "vite",
    "plop": "plop",
    "build": "vue-tsc && vite build",
    "preview": "yarn build && vite preview",
    "typecheck": "vue-tsc",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "postinstall": "simple-git-hooks"
  },
  "dependencies": {
    "@vueuse/core": "^9.3.0",
    "axios": "^1.3.3",
    "element-plus": "^2.2.18",
    "js-cookie": "^3.0.1",
    "lodash-es": "^4.17.21",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.23",
    "pinia-plugin-persistedstate": "^2.3.0",
    "qs": "^6.11.0",
    "vue": "3.3.4",
    "vue-router": "^4.2.1"
  },
  "devDependencies": {
    "@iconify-json/ep": "^1.1.8",
    "@iconify-json/fluent-emoji-flat": "^1.1.8",
    "@iconify/utils": "^2.1.7",
    "@toryz/eslint-config": "^0.1.2",
    "@types/js-cookie": "^3.0.2",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "18.8.0",
    "@types/nprogress": "^0.2.0",
    "@types/qs": "^6.9.7",
    "@vitejs/plugin-legacy": "^4.0.4",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/compiler-sfc": "^3.3.4",
    "autoprefixer": "^10.4.13",
    "chalk": "^5.3.0",
    "eslint": "^8.46.0",
    "lint-staged": "^13.0.3",
    "plop": "^3.1.2",
    "sass": "^1.55.0",
    "simple-git-hooks": "^2.8.1",
    "simply": "^0.0.0",
    "terser": "^5.16.1",
    "typescript": "^5.1.6",
    "unocss": "^0.53.4",
    "unplugin-auto-import": "^0.11.2",
    "unplugin-vue-components": "^0.22.8",
    "vite": "4.3.9",
    "vite-plugin-vue-setup-extend": "^0.4.0",
    "vue-tsc": "^1.7.8"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged && pnpm vue-tsc",
    "commit-msg": "node build/verifyCommit.js"
  },
  "lint-staged": {
    "*": "eslint --fix"
  },
  "browserslist": [
    "last 1 version",
    "> 1%",
    "not dead"
  ]
}
